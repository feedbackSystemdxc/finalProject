<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:black;">
        <div class="container">
            <div class="navbar-header">
                <a href="#Index" style="margin-left:0px"><img src="https://assets.website-files.com/5d96e7e173d7f14b2cdb74f2/5d96e7e173d7f16a59db7a59_3_logo_hz_rev.png" style="width:350px; height:auto; float:left;  margin-left:0px" /></a>

            </div>
            
        </div>
    </div>
    <div class="container body-content" style="margin-top:70px;">
        @RenderBody()
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
